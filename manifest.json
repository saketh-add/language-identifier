{
  "manifest_version": 3,
  "name": "Language Identifier",
  "description": "This extension identifies language of a webpage from html lang attribute",
  "version": "1.0",
  "action": {
    "default_popup": "language.html"
  },
  "icons": {
    "16": "icons/icon_16.png",
    "36": "icons/icon_36.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ]
}